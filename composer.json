{
    "name": "niemand-online/heptaconnect-portal-amiibo",
    "type": "library",
    "description": "HEPTAconnect portal read data from https://amiiboapi.com/",
    "keywords": [
        "heptaconnect-portal",
        "amiibo"
    ],
    "license": [
        "AGPL-3.0-or-later"
    ],
    "authors": [
        {
            "name": "niemand.online"
        }
    ],
    "require": {
        "php": ">=7.4",
        "heptacom/heptaconnect-dataset-ecommerce": ">=0.4",
        "heptacom/heptaconnect-portal-base": ">=0.4"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "require-dev": {
        "dq5studios/psalm-junit": ">=2",
        "ergebnis/composer-normalize": ">=2.7",
        "friendsofphp/php-cs-fixer": ">=2.17",
        "icanhazstring/composer-unused": ">=0.7",
        "infection/infection": ">=0.21",
        "maglnet/composer-require-checker": ">=2",
        "mavimo/phpstan-junit": ">=0.3",
        "phpstan/extension-installer": ">=1",
        "phpstan/phpstan": ">=0.12",
        "phpstan/phpstan-deprecation-rules": ">=0.12",
        "phpstan/phpstan-phpunit": ">=0.12",
        "phpstan/phpstan-strict-rules": ">=0.12",
        "phpunit/phpunit": ">=9.3 <10",
        "psalm/plugin-phpunit": ">=0.10",
        "vimeo/psalm": ">=4"
    },
    "autoload": {
        "psr-4": {
            "NiemandOnline\\HeptaConnect\\Portal\\Amiibo\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "heptaconnect": {
            "portals": [
                "NiemandOnline\\HeptaConnect\\Portal\\Amiibo\\Portal"
            ]
        }
    }
}

